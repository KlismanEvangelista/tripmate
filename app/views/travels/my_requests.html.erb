<h1>Travels#index</h1>
<p>Find me in app/views/travels/index.html.erb</p>
<% @my_requests.each do |r| %>
  <p>
    Plan nombre
    <%= r.plan.name %>
  </p>
  <p>
    Postulante
    <%= r.user.first_name%>
  </p>
  <%= r.status %>
  <%= button_to 'Aceptar solicitud', travel_path(r), method: :patch, class: "btn btn-primary" %>
  <%= link_to 'Eliminar', travel_path(r), data: { turbo_method: :delete, turbo_confirm: "Are you sure?"}, class: "btn btn-danger" %>
<% end %>
